{
    "name": "asgrim/book-library",
    "description": "Demo book library API application for presentations",
    "type": "project",
    "homepage": "https://github.com/asgrim/book-library",
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb"
        }
    ],
    "require": {
        "php": "^7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^2.0.3",
        "zendframework/zend-expressive-helpers": "^4.0",
        "zendframework/zend-stdlib": "^3.1",
        "zendframework/zend-expressive-fastroute": "^2.0",
        "zendframework/zend-servicemanager": "^3.3",
        "ramsey/uuid": "^3.6",
        "dasprid/container-interop-doctrine": "^0.2.3",
        "psr7-sessions/storageless": "dev-feature/psr-http-middleware"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/xtreamwayz/psr7-sessions-storageless"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^2.7",
        "zendframework/zend-expressive-tooling": "^0.4.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit --colors=always"
    }
}
